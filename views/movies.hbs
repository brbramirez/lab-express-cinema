<div class="header">
    <h1>Ironhack Cinema</h1>
    <p>Click on the movie to check the showtimes!</p>
</div>
<br>
<div>
<ul class="movies">
{{#each allMovies}}
<li class="movie">
<img class="img-movies" src="{{this.image}}" alt="{{this.title}}">
<p>{{this.title}}</p>
<button><a href="/movies-details/{{this._id}}">See more</a></button>
</li>
{{/each}}

</ul>
</div>
